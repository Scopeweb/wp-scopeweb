        <section id="hero" class="hero-full text-light parallax-section" data-parallax-image="{{ site.theme.path }}/assets/uploads/ultrabse-casestudy-hero.jpg">

            <div id="page-title" class="wrapper-big">
                <h1>
                    <strong>Ultrabse</strong>
                </h1>
                <div class="spacer-mini"></div>
                <h5>Product Development</h5>
            </div>

        </section>
        <!-- HERO -->

        <!-- PAGEBODY -->
